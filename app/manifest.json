{
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "description": "__MSG_appDescription__",
  "default_locale": "en",
  "version": "0.1.2",
  "icons": {
    "128": "img/icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["js/background.js"]
    }
  },
  "permissions": [
    "storage",
    "https://www.showroom-live.com/",
    "https://image.showroom-live.com/"
  ],
  "sockets": {
    "tcp": {
      "connect": "*:*"
    }
  }
}
